{
  "name": "wedex",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p -l *:watch",
    "compile": "run-s *:compile", 
    "repl": "shadow-cljs browser-repl",
    "cljs:watch": "shadow-cljs watch crx",
    "cljs:compile": "shadow-cljs compile crx",
    "css:build": "cross-env postcss src/css/tailwind.css -o ./public/css/style.css --verbose",
    "css:watch": "cross-env postcss src/css/tailwind.css -o ./public/css/style.css --verbose -w",
    "css:compile": "cross-env NODE_ENV=production postcss src/css/tailwind.css -o ./public/css/main.css --verbose"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "postcss-cli": "^10.1.0",
    "shadow-cljs": "2.25.2",
    "tailwindcss": "^3.3.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "scheduler": "^0.23.0"
  }
}
