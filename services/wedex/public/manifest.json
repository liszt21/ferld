{
  "name": "Wedex",
  "description": "my chrome extension",
  "version": "0.0.0",
  "manifest_version": 3,
  "sandbox": {
    "pages": [
      "side-panel.html",
      "index.html"
    ]
  },
  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "notifications",
    "history",
    "storage",
    "sidePanel",
    "tabs",
    "tabGroups",
    "webRequest",
    "contextMenus"
  ],
  "background": {
    "service_worker": "js/sw.js",
    "type": "module"
  },
  "side_panel": {
    "default_path": "side-panel.html"
  },
  "action": {
    "default_title": "Wedex",
    "default_popup": "popup.html"
  },
  "icons": {
  }
}
